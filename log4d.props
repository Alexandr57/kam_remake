# Set internal debugging
log4d.configDebug=false

# Global logging level
log4d.threshold=debug

# Set logger factory
log4d.loggerFactory=TLogDefaultLoggerFactory

# Set root level
log4d.rootLogger=info,fileLogger

# Establish logger hierarchy
log4d.logger.net=debug
log4d.logger.delivery=inherited

# Create a file appender
log4d.appender.fileLogger=TKMLogFileAppender
log4d.appender.fileLogger.append=false
log4d.appender.fileLogger.fileName=
log4d.appender.fileLogger.layout=TKMLogLayout

